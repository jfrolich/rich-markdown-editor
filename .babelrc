{
  "presets": ["react", "flow", "env"],
  "plugins": [
    "lodash",
    ["emotion", { "hoist": true }],
    "transform-decorators-legacy",
    "transform-es2015-destructuring",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
